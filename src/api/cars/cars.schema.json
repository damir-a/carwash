{
  "$schema": "http://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "created_at",
    "updated_at",
    "deleted_at",
    "uuid",
    "make",
    "makeRU",
    "model",
    "logo"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "default": 0,
      "title": "The id Schema",
      "examples": [
        1
      ]
    },
    "created_at": {
      "type": "string",
      "default": "",
      "title": "The created_at Schema",
      "examples": [
        "2022-05-07 00:06:04"
      ]
    },
    "updated_at": {
      "type": "string",
      "default": "",
      "title": "The updated_at Schema",
      "examples": [
        "2022-05-07 00:06:04"
      ]
    },
    "deleted_at": {
      "type": "string",
      "default": "",
      "title": "The deleted_at Schema",
      "examples": [
        "2022-05-07 00:06:04"
      ]
    },
    "uuid": {
      "type": "string",
      "default": "",
      "title": "The uuid Schema",
      "examples": [
        "49d860ce-ca10-11ec-95c4-0250ef356169"
      ]
    },
    "make": {
      "type": "string",
      "default": "",
      "title": "The make Schema",
      "examples": [
