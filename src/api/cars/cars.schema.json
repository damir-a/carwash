      ]
    },
    "makeRU": {
      "type": "string",
      "default": "",
      "title": "The makeRU Schema",
      "examples": [
        "Ауди"
      ]
    },
    "model": {
      "type": "string",
      "default": "",
      "title": "The model Schema",
      "examples": [
        "A100"
      ]
    },
    "logo": {
      "type": "string",
      "default": "",
      "title": "The logo Schema",
      "examples": [
        "http://audi.com/logo.svg"
      ]
    }
  },
  "examples": [
    {
      "id": 1,
      "created_at": "2022-05-07 00:06:04",
      "updated_at": "2022-05-07 00:06:04",
      "deleted_at": "2022-05-07 00:06:04",
      "uuid": "49d860ce-ca10-11ec-95c4-0250ef356169",
      "make": "Audi",
      "makeRU": "Ауди",
      "model": "A100",
      "logo": "http://audi.com/logo.svg"
    }
  ]
