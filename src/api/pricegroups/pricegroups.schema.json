{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The orders",
  "description": "The orders schema.",
  "default": {},
  "examples": [
    {
      "client_id": 1,
      "order_id": 1,
      "service_id": 1,
      "user_id": 1,
      "car_id": 1
    }
  ],
  "required": [
    "title"
  ],
  "properties": {
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The price group title",
      "description": "Price group title in DB.",
      "default": ""
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The price group desription",
      "description": "Price group desription in DB.",
      "default": ""
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "id",
      "description": "id in DB.",
      "default": 0,
      "examples": [
        1
      ]
    },
    "deleted_at": {
      "$id": "#/properties/deleted_at",
      "type": "string",
      "title": "Time of order deletion",
      "description": "Time of order deletion."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "Time of order updation",
      "description": "Time of order updation."
    }
  },
  "additionalProperties": false
}